<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.photo.selectlib">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name="com.photo.selectlib.activity.BaseActivity"></activity>
        <activity android:name="com.photo.selectlib.activity.ClipImageActivity" />
        <activity android:name="com.photo.selectlib.activity.FolderListActivity" />
        <activity android:name="com.photo.selectlib.activity.ImageSelectActivity" />
        <activity android:name="com.photo.selectlib.activity.PreviewImageActivity" />
    </application>

</manifest>